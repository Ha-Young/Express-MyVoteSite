extends layouts/main

block content
	.result-container
		.result-image-container
		.result-message-container
			span.result-message=message
		.result-buttons-container
			.result-button-container
				a(href="/").result-button.result-button-home 목록으로 돌아가기
			.result-button-container
				if result === "success"
					a(href="/votings/" + votingId).result-button.result-button-detail 생성한 투표 보러가기
