extends layout

block content
  main
    h1= title
    p Welcome to voting-app
    if loginUser
      button.button-vote-creator 내투표 만들기
    -var values = [{title:"title", creator:"creator", due:"2020-03-20 3pm", state:"continue"   }];
      each val in values
        ul.vote
          li.vote-title=val.title
          li.vote-creator=val.creator
          li.vote-due=val.due
          li.vote-state=val.state
