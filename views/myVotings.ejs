<div class="body-container">
  <h1>내 투표</h1>
  <div class="votes-container">
    <% if (votes.length > 0) { %>
      <% votes.map((vote) => { %>
        <div class="vote">
          <h1><%= vote.title %></h1>
          <div>
            <span>마감일 : </span>
            <span><%= vote.dueDate %></span>
          </div>
        </div>
      <% }) %>
    <% } else { %>
      <div>No votes..😥</div>
    <% }%>
  </div>
</div>
