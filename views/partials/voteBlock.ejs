<a class="vote-box" href="<%= `/votings/${vote._id}` %>">
  <p class="title">
    <%= vote.title %>
  </p>
  <ul>
    <li class="author">
      Created by <%= vote.author.nickname %>
    </li>
    <li class="expiration_date">
      expired at <%= vote.expirationDate %>
    </li>
    <li class="status">
      <%= vote.status %>
    </li>
  </ul>
</a>
