each poll in voting.polls
  div(class='poll')
    a(href='/votings/#{voting._id}/polls/#{poll._id}')
      ul
        li= poll.title
        -  let value = '';
        -  if (isCreator || !voting.openState) {
        -    value = poll.voted_users.length;
        -  } else {
        -    value = '?'
        -  }
        -
        li #{value}
